<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>DOSClientServlet</servlet-name>
    <servlet-class>DOSPackage.DOSClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>DOSPackage.WelcomeServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>DOSAttack</servlet-name>
    <servlet-class>DOSPackage.DOSAttack</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>DOSClientServlet</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/servlet2</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
   <servlet-name>DOSAttack</servlet-name>
    <url-pattern>/servlet3</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>